    "rimraf": "^4.1.2",
    "rxjs": "^7.5.7",
    "simple-oauth2": "^5.0.0",
    "thirty-two": "^1.0.2",
    "typed-rest-client": "^1.8.9",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/busboy": "^1.5.0",
    "@types/connect-pg-simple": "^7.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/qrcode": "^1.5.0",
    "@types/simple-oauth2": "^4.1.1",
